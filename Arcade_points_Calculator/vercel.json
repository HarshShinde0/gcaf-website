{
  "version": 2,
  "builds": [
    {
      "src": "Arcade_points_Calculator/asgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "Arcade_points_Calculator/asgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "Arcade_points_Calculator/asgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "Arcade_points_Calculator.settings",
    "PYTHONPATH": "/vercel/path0/Arcade_points_Calculator"
  }
}
